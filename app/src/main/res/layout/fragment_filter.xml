<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/ms_white"
    tools:context=".ShowFilter">


    <com.jaygoo.widget.RangeSeekBar
        android:layout_width="match_parent"
        android:layout_height="29dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="286dp"
        android:layout_marginEnd="20dp"
        app:rsb_indicator_arrow_size="2dp"
        app:rsb_max="10000"
        app:rsb_min="10"
        app:rsb_mode="range"
        app:rsb_progress_color="@color/buttons" />


    <ImageButton
        android:id="@+id/decreaseR"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="244dp"
        android:layout_marginTop="392dp"
        android:layout_marginEnd="124dp"
        android:background="@color/float_transparent"
        app:srcCompat="@drawable/ic_negative_vector_40" />


    <ImageButton
        android:id="@+id/increaseR"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="343dp"
        android:layout_marginTop="392dp"
        android:layout_marginEnd="25dp"
        android:background="@color/float_transparent"
        app:srcCompat="@drawable/ic_plus_vector_40" />

    <TextView
        android:id="@+id/numofRooms"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="294dp"
        android:layout_marginTop="391dp"
        android:layout_marginEnd="31dp"
        android:gravity="center"
        android:text="1"
        android:textAlignment="center"
        android:textColor="@color/ms_black"
        android:textSize="22sp" />


    <ImageButton
        android:id="@+id/decreaseB"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="244dp"
        android:layout_marginTop="446dp"
        android:layout_marginEnd="124dp"
        android:background="@color/float_transparent"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/ic_negative_vector_40" />


    <ImageButton
        android:id="@+id/increaseB"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="343dp"
        android:layout_marginTop="447dp"
        android:layout_marginEnd="25dp"
        android:background="@color/float_transparent"
        app:srcCompat="@drawable/ic_plus_vector_40" />

    <TextView
        android:id="@+id/numofbathroom"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="294dp"
        android:layout_marginTop="446dp"
        android:layout_marginEnd="31dp"
        android:gravity="center"
        android:text="1"
        android:textAlignment="center"
        android:textColor="@color/ms_black"
        android:textSize="22sp" />

    <Button
        android:id="@+id/city"
        style="@style/Widget.MaterialComponents.Chip.Filter"
        android:layout_width="match_parent"
        android:layout_height="95dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="81dp"
        android:layout_marginBottom="299dp"
        android:background="@color/white_transparency"
        android:drawableEnd="@drawable/ic_arrow_right_16px"
        android:hint="Amman"
        android:paddingStart="18dp"
        android:paddingTop="15dp"
        android:paddingEnd="12dp"
        android:text="Amman"
        android:textAlignment="textStart"
        android:textAppearance="@style/Widget.MaterialComponents.Chip.Filter"
        android:textColor="@color/buttons"
        android:textColorHint="@color/gray_btn_bg_color"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="87dp"
        android:clickable="true"
        android:paddingStart="12dp"
        android:text="LOCATION"
        android:textAppearance="@style/TextAppearance.AppCompat.Caption" />


    <Button
        android:id="@+id/date"
        style="@style/Widget.MaterialComponents.Chip.Filter"
        android:layout_width="match_parent"
        android:layout_height="95dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="176dp"
        android:background="@color/white_transparency"
        android:drawableEnd="@drawable/ic_arrow_right_16px"
        android:hint="Amman"
        android:paddingStart="18dp"
        android:paddingTop="15dp"
        android:paddingEnd="12dp"
        android:text="2019"
        android:textAlignment="textStart"
        android:textAppearance="@style/Widget.MaterialComponents.Chip.Filter"
        android:textColor="@color/buttons"
        android:textColorHint="@color/gray_btn_bg_color"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="183dp"
        android:paddingStart="12dp"
        android:text="DATE"
        android:textAppearance="@style/TextAppearance.AppCompat.Caption" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="458dp"
        android:fontFamily="@font/tajawallight"
        android:text="Number of BathRooms"
        android:textColor="@color/ms_black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="404dp"
        android:layout_marginBottom="237dp"
        android:fontFamily="@font/tajawallight"
        android:text="Number of Rooms"
        android:textColor="@color/ms_black"
        android:textSize="20sp" />

    <CheckBox
        android:id="@+id/parkingAsk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="346dp"
        android:layout_marginTop="522dp"
        android:layout_marginEnd="31dp"
        android:buttonTint="@color/buttons"
        android:checked="false"
        android:scaleX="1.30"
        android:scaleY="1.30"
        android:textColor="@color/ms_black"
        android:textSize="12sp" />


    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="524dp"
        android:fontFamily="@font/tajawallight"
        android:text="Parking ?"
        android:textColor="@color/ms_black"
        android:textSize="20sp"
                />

    <View
        style="@style/separator"
        android:layout_width="362dp"
        android:layout_height="1dp"

        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="574dp"
        android:layout_marginEnd="25dp" />

    <TextView
        android:id="@+id/apply"
        android:layout_width="111dp"
        android:layout_height="34dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="283dp"
        android:layout_marginTop="601dp"
        android:layout_marginEnd="18dp"
        android:fontFamily="@font/tajawalmedium"
        android:gravity="center"
        android:text="Apply"
        android:textColor="@color/buttons"
        android:textSize="22sp" />

    <TextView
        android:id="@+id/reset"
        android:layout_width="111dp"
        android:layout_height="34dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="22dp"
        android:layout_marginTop="601dp"
        android:layout_marginEnd="277dp"
        android:clickable="true"
        android:fontFamily="@font/tajawalmedium"
        android:gravity="center"
        android:text="Reset"
        android:textColor="#ECF0F1"
        android:textSize="22sp" />


</RelativeLayout>